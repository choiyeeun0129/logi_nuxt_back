<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.seoulit.erp.account.base.dao.BusinessDAO">
 

    <select id="selectBusinessList" resultType="kr.co.seoulit.erp.account.base.to.BusinessBean">
    
        SELECT BUSINESS_NAME as businessname,
        	   CLASSIFICATION_CODE as classificationcode,
        	   REMARKS as remarks
        FROM BUSINESS 
        ORDER BY CLASSIFICATION_CODE
        
    </select>
</mapper>